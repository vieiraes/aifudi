{
  "editor.fontFamily": "'JetBrains Mono'",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
  },
  "explorer.autoRevealExclude": {
    "**/node_modules": true,
    "**/bower_components": true
  },
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#95b48d",
    "activityBar.background": "#95b48d",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#65709a",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#95b48d",
    "statusBar.background": "#78a06e",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#608457",
    "statusBarItem.remoteBackground": "#78a06e",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#78a06e",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#78a06e99",
    "titleBar.inactiveForeground": "#15202b99",
    "peacock.color": "#78a06e",
  }
}